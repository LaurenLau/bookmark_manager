<ul>
  <% @bookmarks.each do |bookmark| %>
    <li>
      <a href=<%= bookmark.url %> >
        <%= bookmark.title %>
      </a>
    </li>
  <% end %>
  
</ul>

<form action='/add_bookmark' method='post' >
  <input type= 'text' value = 'title' name = 'title'>
  <input type= 'text' value = 'url' name = 'url'>
  <input type= 'submit' value='add' name = 'add'>
</form>
